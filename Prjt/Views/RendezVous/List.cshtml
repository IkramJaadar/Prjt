@model List<RendezVous>
<section class="breadcrumb breadcrumb_bg">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="breadcrumb_iner text-center">
                    <div class="breadcrumb_iner_item">
                        <h2>Liste des RendezVous</h2>

                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="blog_area section_padding">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <table class="table table-striped">
                            <thead>
                                <tr>
                                    <th> Horraire</th>
                                    <th> Date </th>
                                    <th> Etat </th>
                                    <th> Nom du Service </th>
                                    <th> Prix</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (RendezVous rd in Model)
                                {
                                    <tr>
                                        <td> @rd.Horraire</td>
                                        <td>@rd.Date.ToString("dd-MM-yyyy")</td>
                                        <td> @rd.Etat </td>
                                        <td> @rd.services.NomS </td>
                                        <td> @rd.payement.Prix </td>

                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>